{:paths   ["src/shared" "resources" "src/patch"]

 :classpath-overrides {org.clojure/clojurescript ""}

 :deps    {
           ;org.clojure/clojure                    {:mvn/version "1.10.3"}
           org.clojure/clojure                    {:mvn/version "1.11.1"}
           org.clojure/clojurescript              {:git/url "https://github.com/clojure/clojurescript"
                                                   :git/sha "89b4c19005cbeebb08649c3e60dc3e1524adccda"}
           com.fulcrologic/fulcro                 {:mvn/version "3.5.24"}
